{
  "name": "SliseBackend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prisma:migrate:up && npm run prisma:generate && nest start",
    "start:watch": "npm run prisma:migrate:up && npm run prisma:silent-watch && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prisma:migrate:up": "npm run prisma -- migrate deploy",
    "prisma:generate": "npm run prisma -- generate",
    "prisma:watch": "npm run prisma -- generate -- --watch",
    "prisma:silent-watch": "npm run prisma -- generate -- --watch &",
    "seed": "ts-node --transpile-only ./prisma/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:prod": "node dist/src/main.js"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^6.0.1",
    "@nestjs-modules/ioredis": "^1.0.0",
    "@nestjs/axios": "0.0.8",
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^3.15.2",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.5.1",
    "@sentry/tracing": "^7.4.1",
    "@zoralabs/zdk": "^2.1.3",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ethereum-block-by-date": "^1.4.5",
    "etherscan-api": "^10.0.9",
    "ioredis": "^4.28.5",
    "moralis": "^1.8.1",
    "oraq": "^0.2.4",
    "pg": "^8.7.3",
    "postgres": "^3.2.4",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "web3": "^1.7.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^3.15.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
